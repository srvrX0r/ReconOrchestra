OPENAI_API_KEY=REPLACE_ME_LOCAL_DEV
LLM_PROVIDER=openai
LLM_MODEL=gpt-5-thinking-mini
TOOLS_PATH=/usr/local/bin

# Tool command templates (do not include secrets)
AMASS_CMD="amass enum -passive -d {domain} -json -"
SUBFINDER_CMD="subfinder -d {domain} -silent -oJ -"
HTTPX_CMD="httpx -l {input_file} -silent -json -o {out_file} --follow-redirects"
NUCLEI_CMD="nuclei -l {input_file} -json -o {out_file} -severity critical,high,medium -tags safe"

OUTPUT_DIR=orchestrator_output
